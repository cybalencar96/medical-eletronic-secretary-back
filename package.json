{
  "name": "medical-eletronic-secretary-back",
  "version": "1.0.0",
  "description": "WhatsApp-based medical appointment scheduling system with AI-powered intent recognition",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "TZ=America/Sao_Paulo jest --coverage --testPathIgnorePatterns=/node_modules/ --testPathIgnorePatterns=tests/integration",
    "test:watch": "TZ=America/Sao_Paulo jest --watch",
    "test:integration": "TZ=America/Sao_Paulo jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make"
  },
  "keywords": [
    "whatsapp",
    "medical",
    "appointment",
    "scheduling",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^6.16.4",
    "@bull-board/express": "^6.16.4",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-http": "^8.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
